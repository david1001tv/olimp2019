{
  "presets": [
    "stage-1",
    "env",
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-decorators-legacy",
    "transform-export-extensions",
    [
      "transform-imports",
      {
        "react-md": {
          "transform": "../../transform.js",
          "preventFullImport": true
        }
      }
    ]
  ]
}